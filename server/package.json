    "fastify": "^4.10.2",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "read-excel-file": "^5.5.3",
    "sequelize": "^6.27.0",
    "sequelize-cli": "^6.5.2",
    "sequelize-mig": "github:sacredneobi/sequelize-mig",
    "telegraf": "^4.11.2"
  }
